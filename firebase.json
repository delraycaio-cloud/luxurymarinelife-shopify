{
  "hosting": {
    "site": "luxurymarinelife-shopify",
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/ultra-shear-classic",
        "destination": "/ultra-shear-classic.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
